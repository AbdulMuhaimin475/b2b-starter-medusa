{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": [".next/**", "!.next/cache/**"]
    },
    "dev": {
      "dependsOn": ["^build-packages"],
      "persistent": true,
      "cache": false,
      "env": ["NEXT_PUBLIC_MEDUSA_PUBLISHABLE_KEY"]
    }
  }
}
